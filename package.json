{
  "name": "oracle23ai-fhir-lab",
  "version": "1.0.0",
  "description": "A modern Node.js TypeScript project with ESM support",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/consumer/index.js",
    "dev": "tsx watch src/consumer/index.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest --config ./vitest.config.ts",
    "test:coverage": "vitest --config ./vitest.config.ts --coverage"
  },
  "keywords": [
    "typescript",
    "node",
    "esm"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.7.0",
    "@types/oracledb": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "luxon": "^3.7.2",
    "nats": "^2.29.3",
    "openai": "^6.5.0",
    "oracledb": "^6.10.0",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  }
}
